{
    "extends": ["prettier", "react-app"],
    "plugins": ["react-hooks", "babel"],
    "parser": "babel-eslint",
    "globals": {
        "React": true
    },
    "rules": {
        "arrow-spacing": [
            2,
            {
                "before": true,
                "after": true
            }
        ],
        "no-duplicate-case": 2,
        "handle-callback-err": [2, "^(err|error)$"],
        // "react/prop-types": [
        //     2,
        //     {
        //         "ignore": ["children"]
        //     }
        // ],
        "no-var": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "strict": [2, "never"],
        "no-invalid-regexp": 2,
        "valid-typeof": 2,
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "none"
            }
        ],
        "no-useless-call": 2,
        "no-unused-expressions": "off",
        "babel/no-unused-expressions": "error",
        "no-console": 0,
        "no-constant-condition": 0,
        "jsx-quotes": ["warn", "prefer-double"]
    }
}
